<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>Jokes!</title>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
  </head>
  <body>
    <header>
      <h1>Super jokes!</h1>
    </header>
    <main>
      <section>
        <h2>List of jokes</h2>
        <% for(const joke of jokes) { %>
        <article>
          <p>Question: <%= joke.question %></p>
          <p>Answer: <%= joke.answer %></p>
          <p>Rating: <%= joke.rating %></p>
          <p>By: <%= joke.name %></p>
        </article>
        <% } %>
      </section>
    </main>
  </body>
</html>
